
class Machine {
	public void start() {
		System.out.println("Machine started.");
	}
}

public class App {

	public static void main(String[] args) {
	
		Machine mach1 = new Machine() {
			@Override public void start() {
				System.out.println("Camera starting");
			}
		};
		
		mach1.start();
		
	}

}
